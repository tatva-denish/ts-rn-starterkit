{
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./app"
        ],
      "alias": {
         "@app": "./app",
         "@config":"./app/config",
         "@utils":"./app/utils",
         "@screens":"./app/screens",
         "@components":"./app/components",
         "@models":"./app/models",
         "@services":"./app/services",
         "@theme":"./app/theme",
         "@context":"./app/context",
         "@lang":"./app/i18n"
       }
      } 
    ] 
  ]
}